  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": "use yarn instead",
    "yarn": "^1.5"
  },
  "resolutions": {
    "cheerio": "1.0.0-rc.12",
    "@mdx-js/react": "^3.1.0"
  }
}
